{
  "manifest_version": 3,
  "name": "Bless Auto Installer",
  "version": "1.0",
  "description": "Auto installer for Bless extension",
  "permissions": [
    "management",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://chromewebstore.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
